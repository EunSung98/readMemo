function check() {
	if(!document.signUpForm.name.value){
		alert("이름을 입력해주세요");
		form.name.focus();
		return false;
	}else if (!document.signUpForm.id.value) {
		alert("아이디를 입력해주세요");
		form.id.focus();
		return false;
	} else if (!document.signUpForm.pw.value){
		alert("비밀번호를 입력해주세요");
		form.pw.focus();
		return false;
	}
	return true;
}